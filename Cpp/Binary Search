# include <iostream>
using namespace std;

int main()
{
    int n;
    cout << "Enter the number to be searched: ";
    cin >> n;
    
    int size;
    cout << "Enter size of array: ";
    cin >> size;
    
    cout << "Enter the array in ascending order: ";
    int a[size];
    for(int i=0; i<size; i++)
        cin >> a[i];
    
    bool f = false;
    int mid, first = 0, last = n-1;
    while(first<=last)
    {
        mid = (first+last) / 2;
        if(a[mid]==n)
        {
            f = true;
            break;
        }
        if(a[mid]<n)
            last = mid-1;
        else if(a[mid]>n)
            first = mid+1;
    }
    if(f)
        cout << "Found";
    else
        cout << "Not Found";
}
