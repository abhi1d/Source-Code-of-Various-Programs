a = eval(input("Enter a number: "))
l = input('Enter the list of items separated by a space: ')
p = l.split()
for f in range(len(p)):
    p[f] = eval(p[f])
p.sort()
first = 0
last = len(p)-1
while first <= last:
    mid = int((first+last)/2)
    if a == p[mid]:
        print("Found")
        return
    if a < p[mid]:
        last = mid -1
    elif a > p[mid]:
        first = mid+1
print('Not found')
